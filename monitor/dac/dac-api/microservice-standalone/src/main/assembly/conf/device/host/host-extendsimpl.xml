<extensions>
	<!-- IDataCollector Impl mapping -->
	<extension-point id="org.openo.nfvo.monitor.dac.dataaq.common.IDataCollector">
		<impl type="nfv.host.linux" class="org.openo.nfvo.monitor.dac.dataaq.datacollector.UnixPerformDataCollector"/>
	</extension-point>
	
	<!-- IDataParser Impl mapping -->
	<extension-point id="org.openo.nfvo.monitor.dac.dataaq.common.IDataParser">
		<impl type="nfv.host.linux" class="org.openo.nfvo.monitor.dac.dataaq.dataparser.TelnetDataParser"/>
	</extension-point>
	
	<!-- IMonitor Impl mapping -->
	<extension-point id="org.openo.nfvo.monitor.dac.dataaq.common.IMonitor">
		<impl type="nfv.host.linux" class="org.openo.nfvo.monitor.dac.dataaq.monitor.MonitorTelnet"/>
	</extension-point>
	
	<!-- IMonitorTaskInfo Impl mapping -->
	<extension-point id="org.openo.nfvo.monitor.dac.dataaq.monitor.bean.common.IMonitorTaskInfo">
		<impl type="nfv.host.linux" class="org.openo.nfvo.monitor.dac.dataaq.monitor.bean.taskinfo.HostTaskInfo"/>
	</extension-point>
</extensions>